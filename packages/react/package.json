{
  "name": "@akello/react",
  "private": false,
  "version": "0.0.23",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgnacioNMiranda/vite-component-library-template"
  },
  "main": "./dist/react.umd.js",
  "module": "./dist/react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react.es.js",
      "require": "./dist/react.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@akello/core": "../core",
    "@akello/react-hook": "^0.0.3",
    "@mantine/core": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.7.2",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "tailwindcss": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.1.6",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@heroicons/react": "^2.1.3",
    "@mantine/modals": "^7.7.1",
    "@mantine/notifications": "^7.7.1",
    "@modulz/radix-icons": "^4.0.0",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^7.1.0",
    "@tabler/icons-react": "^3.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "formik": "^2.4.5",
    "husky": "^9.0.11",
    "moment": "^2.30.1",
    "react-tailwindcss-datepicker": "^1.6.6",
    "recharts": "^2.12.3",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "vite": "^5.2.7",
    "vite-plugin-dts": "^3.8.1",
    "vitest": "^1.4.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.0",
    "@storybook/addon-essentials": "^8.0.5",
    "@storybook/addon-interactions": "^8.0.5",
    "@storybook/addon-links": "^8.0.5",
    "@storybook/addon-onboarding": "^8.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-vite": "^8.0.5",
    "@storybook/test": "^8.0.5",
    "@types/react": "^18.2.74",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^9.0.8",
    "eslint-plugin-storybook": "^0.8.0",
    "prop-types": "^15.8.1",
    "storybook": "^8.0.5"
  }
}
