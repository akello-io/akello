{
  "name": "@akello/react",
  "private": false,
  "version": "2.0.4",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgnacioNMiranda/vite-component-library-template"
  },
  "main": "./dist/react.umd.js",
  "module": "./dist/react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react.es.js",
      "require": "./dist/react.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "peerDependencies": {
    "@akello/core": "../core",
    "@akello/react-hook": "../react-hook",
    "@mui/icons-material": "^5.15.6",
    "@mui/lab": "^5.0.0-alpha.162",
    "@mui/material": "^5.15.7",
    "@mui/styled-engine": "^5.15.7",
    "@mui/styled-engine-sc": "^6.0.0-alpha.13",
    "@mui/x-data-grid": "^6.19.3",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.6.1",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@types/react": "^18.2.52",
    "classnames": "^2.5.1",
    "formik": "^2.4.5",
    "husky": "^9.0.10",
    "recharts": "^2.11.0",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2",
    "yup": "^1.3.3"
  }
}
