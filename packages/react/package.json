{
  "name": "@akello/react",
  "private": false,
  "version": "0.0.20",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgnacioNMiranda/vite-component-library-template"
  },
  "main": "./dist/react.umd.js",
  "module": "./dist/react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react.es.js",
      "require": "./dist/react.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "peerDependencies": {
    "@akello/core": "^0.0.7",
    "@akello/react-hook": "^0.0.1",
    "@mantine/core": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@mui/icons-material": "^5.15.6",
    "@mui/lab": "^5.0.0-alpha.162",
    "@mui/material": "^5.15.10",
    "@mui/styled-engine": "^5.15.9",
    "@mui/styled-engine-sc": "^6.0.0-alpha.13",
    "@mui/x-data-grid": "^6.19.4",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.7.2",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.16.0",
    "tailwindcss": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@akello/core": "file:../../packages/core",    
    "@akello/react-hook": "file:../../packages/react-hook",
    "@heroicons/react": "^2.1.1",
    "@mantine/core": "^7.5.3",
    "@mantine/hooks": "^7.5.3",
    "@mui/material": "^5.15.10",
    "@mui/styled-engine": "^5.15.9",
    "@mui/x-data-grid": "^6.19.4",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/react-vite": "^7.6.16",
    "autoprefixer": "^10.4.17",
    "daisyui": "^4.7.2",
    "husky": "^9.0.11",
    "postcss": "^8.4.35",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "storybook": "^7.6.16",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.18",
    "@tabler/icons-react": "^2.47.0",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.56",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "formik": "^2.4.5",
    "json": "^11.0.0",
    "recharts": "^2.12.0",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.3.0",
    "yup": "^1.3.3"
  }
}
