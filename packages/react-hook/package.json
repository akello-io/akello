{
  "name": "@akello/react-hook",
  "private": false,
  "version": "0.0.1",
  "packageManager": "pnpm@8.4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IgnacioNMiranda/vite-component-library-template"
  },
  "main": "./dist/react-hook.umd.js",
  "module": "./dist/react-hook.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-hook.es.js",
      "require": "./dist/react-hook.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build",
    "build:lib": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,mdx}\"",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "prepare": "husky install",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "peerDependencies": {
    "@akello/core": "^0.0.7",
    "amazon-cognito-identity-js": "^6.3.7",
    "aws-amplify": "^6.0.13",
    "axios": "^1.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@akello/core": "file:../../packages/core",
    "@heroicons/react": "^2.1.1",
    "@storybook/react": "^7.6.16",
    "@storybook/react-vite": "^7.6.16",
    "husky": "^9.0.11",
    "storybook": "^7.6.16",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@types/react": "^18.2.56",
    "@vitejs/plugin-react": "^4.2.1",
    "amazon-cognito-identity-js": "^6.3.7",
    "classnames": "^2.5.1",
    "json": "^11.0.0",
    "vite": "^5.1.3",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.3.0"
  }
}
