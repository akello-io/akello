    "check": "../../../scripts/prepare/check.ts",
    "prep": "../../../scripts/prepare/bundle.ts"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "jest-specific-snapshot": "^8.0.0",
    "jscodeshift": "^0.14.0",
    "typescript": "~4.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts"
